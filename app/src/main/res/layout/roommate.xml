<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.v7.widget.CardView
        android:id="@+id/card_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="2dp"
        android:foreground="?android:attr/selectableItemBackground"
        app:cardBackgroundColor="@android:color/white"
        app:cardCornerRadius="4dp"
        app:cardElevation="4dp">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_marginBottom="0dp">




        <TextView
            android:id="@+id/rmmMunicipality"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:layout_alignBaseline="@+id/textView19"
            android:layout_alignBottom="@+id/textView19"
            android:layout_alignRight="@+id/view5"
            android:layout_alignEnd="@+id/view5" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView20"
            android:layout_alignStart="@+id/textView20"
            android:layout_below="@+id/view5"
            android:layout_marginTop="14dp"
            android:text="Општина"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/rmmTitle"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/rmmImage"
            android:layout_marginLeft="12dp"
            android:layout_toEndOf="@+id/rmmImage"
            android:layout_toRightOf="@+id/rmmImage"
            android:text="TextView"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/rmmImage"
            android:layout_width="60dp"
            android:layout_height="60dp"
            app:srcCompat="@drawable/male"
            android:layout_marginTop="12dp"
            android:layout_alignParentTop="true"
            android:layout_alignLeft="@+id/view"
            android:layout_alignStart="@+id/view" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12dp"
            android:layout_marginLeft="20dp"
            android:layout_marginStart="20dp"
            android:text="Име"
            android:textStyle="bold"
            android:layout_above="@+id/textView12"
            android:layout_alignLeft="@+id/view"
            android:layout_alignStart="@+id/view" />

        <TextView
            android:id="@+id/rmmName"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:layout_alignBaseline="@+id/textView8"
            android:layout_alignBottom="@+id/textView8"
            android:layout_alignRight="@+id/view"
            android:layout_alignEnd="@+id/view" />

        <View
            android:id="@+id/view"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="130dp"
            android:background="#000000" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView8"
            android:layout_alignStart="@+id/textView8"
            android:layout_below="@+id/view"
            android:layout_marginTop="14dp"
            android:text="Цена ( од - до )"
            android:textStyle="bold" />

        <View
            android:id="@+id/view1"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="175dp"
            android:background="#000000" />

        <TextView
            android:id="@+id/rmmPrice"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/rmmName"
            android:layout_alignStart="@+id/rmmName"
            android:layout_alignTop="@+id/textView12"
            android:text="TextView" />


        <TextView
            android:id="@+id/textView14"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView12"
            android:layout_alignStart="@+id/textView12"
            android:layout_below="@+id/view1"
            android:layout_marginTop="13dp"
            android:text="м2 ( од - до )"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/rmmSqMeters"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/rmmPrice"
            android:layout_alignStart="@+id/rmmPrice"
            android:layout_alignTop="@+id/textView14"
            android:text="TextView" />
        <View
            android:id="@+id/view2"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="220dp"
            android:background="#000000" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView14"
            android:layout_alignStart="@+id/textView14"
            android:layout_below="@+id/view2"
            android:layout_marginTop="12dp"
            android:text="Посебна соба"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/rmmSeparateRoom"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView16"
            android:layout_alignLeft="@+id/rmmSqMeters"
            android:layout_alignStart="@+id/rmmSqMeters"
            android:text="TextView" />

        <View
            android:id="@+id/view3"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="265dp"
            android:background="#000000" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView16"
            android:layout_alignStart="@+id/textView16"
            android:layout_below="@+id/view3"
            android:layout_marginTop="15dp"
            android:text="Креиран на"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/rmmDate"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/rmmSeparateRoom"
            android:layout_alignStart="@+id/rmmSeparateRoom"
            android:layout_alignTop="@+id/textView18"
            android:text="TextView" />

        <View
            android:id="@+id/view4"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="310dp"
            android:background="#000000" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView18"
            android:layout_alignStart="@+id/textView18"
            android:layout_below="@+id/view4"
            android:layout_marginTop="16dp"
            android:text="Телефон"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/rmmPhone"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/rmmDate"
            android:layout_alignStart="@+id/rmmDate"
            android:layout_alignTop="@+id/textView20"
            android:text="TextView" />

        <View
            android:id="@+id/view5"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_below="@+id/rmmPhone"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="15dp"
            android:background="#000000" />

        <TextView
            android:id="@+id/rmmDescription"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/view6"
            android:layout_alignLeft="@+id/textView19"
            android:layout_alignRight="@+id/view6"
            android:layout_alignStart="@+id/textView19"
            android:layout_below="@+id/view6"
            android:layout_marginBottom="15dp"
            android:layout_marginTop="15dp"
            android:text="TextView" />

        <View
            android:id="@+id/view6"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_below="@+id/rmmMunicipality"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="15dp"
            android:background="#000000" />


    </RelativeLayout>
    </android.support.v7.widget.CardView>



</LinearLayout>